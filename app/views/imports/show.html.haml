%h2= l(:label_import_issues)
- if @import.unsaved_items.count > 0
  %p= l(:notice_import_finished, :count => @import.saved_items.count)
  %ul
    - @import.saved_objects.each do |issue|
      %li= link_to_issue issue
- if @import.unsaved_items.count > 0
  %p= l(:notice_import_finished_with_errors, :count => @import.unsaved_items.count, :total => @import.total_items)
  %table#unsaved-items.list
    %tr
      %th Position
      %th Message
    - @import.unsaved_items.each do |item|
      %tr
        %td= item.position
        %td= simple_format_without_paragraph item.message
- content_for :sidebar do
  = render :partial => 'issues/sidebar'
